{
  "manifest_version": 2,
  "name": "ZLB",
  "description": "zhihu live block",
  "version": "1.0",
  "content_scripts": [{
      "matches": ["*://*.zhihu.com/*"],
      "js": ["zlb.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*.zhihu.com/*"
  ],
  "web_accessible_resources": [
    "*.js"
  ]
}
